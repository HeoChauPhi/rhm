{% extends 'layout.twig' %}

{% block feature %}
  {% if post.get_field('feature_banner_fields') %}
    <div class="feature">
      {{ flexible_content('feature_banner_fields') }}
    </div>
  {% endif %}
{% endblock %}

{% block content %}

  {% for post in posts %}
    {% include ["single-"~post.post_name~".twig", "single-"~post.post_type~".twig", 'tease.twig'] %}
  {% endfor %}
{% endblock %}
